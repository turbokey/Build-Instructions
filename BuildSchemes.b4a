Sub InitScrollView(str As String, nom As Int)
	Dim i As Int
	Dim TextReader1 As TextReader
	bykva=str
	scvImages.Panel.RemoveAllViews
	Instruction_is_open = True
	If GetDefaultLanguage="en" Then
    TextReader1.Initialize(File.OpenInput(File.DirAssets, str&"_eng.txt"))
	Else
	TextReader1.Initialize(File.OpenInput(File.DirAssets, str&".txt"))	
	End If
	fullscreen.Initialize(Null)
	fullscreen.Gravity=Gravity.FILL
	scvImages.Visible=True
	For i=0 To nom
		Dim imvImage As ImageView																			
		Dim bmpImage As Bitmap
		bmpImage.Initialize(File.DirAssets, str&i&".jpg")
'		bmpImage.Initialize(File.DirInternal,"Image"&i&".jpg")
'		bmpImage.Initialize(File.DirDefaultExternal,"Image"&i&".jpg")
		imvImage.Initialize("imvImage")
		imvLabel.Initialize("imvLabel")
		imvLabel.Text=TextReader1.ReadLine
		imvLabel.TextColor=Colors.White
		imvLabel.Color = Colors.RGB(5,0,80)
		imvImage.Gravity=Gravity.FILL
		imvImage.Tag=i
		imvImage.Bitmap=bmpImage
		scvImages.Panel.AddView(imvImage,0,i*(imgHeight+imgSpace+lblHeight)+40dip,imgWidth,imgHeight)
		scvImages.Panel.AddView(imvLabel,0,imvImage.Top+imgHeight,imgWidth,lblHeight)
		SetLabelTextSize(imvLabel,imvLabel.Text, maxs,1)
	Next
	scvImages.ScrollPosition=0
	scvImages.Panel.Height=(nom+1)*(imgHeight+imgSpace+lblHeight)+40dip
	scvImages.Left=0
	scvImages.Top=15%y
	scvImages.Width=100%x
	scvImages.Height=85%y-90dip
	TextReader1.Close
End Sub

