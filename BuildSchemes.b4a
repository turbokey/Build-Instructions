	    Else
		KillApp = Msgbox2("Вы уверены, что хотите выйти?","Выход", "Да", "", "Нет", Null)	
		End If
		If KillApp = DialogResponse.POSITIVE Then
	        ExitApplication
			Return False
	    Else
			'SlidingMenu.BarsOff
	        Return True
	    End If
	End If
	End If
	'Return KeyCode	
End Sub
